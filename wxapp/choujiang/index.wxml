<view class="container-out">
        <view class="circle" 
        wx:for="{{circleList}}" wx:key="index">
        </view>
        <view class="container-in">
          <view class="content-out"
           wx:for="{{awardList}}">
            <image class="award-image"
             src="{{item.imageAward}}" />
          </view>
          <view class="start-btn" bindtap="startGame" >START</view>
        </view>
      </view>
      